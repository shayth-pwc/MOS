<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:form="sap.ui.layout.form" controllerName="pwc.portal.eval.ClubEvaluations.view.Applications.AccountingPrograms43G.controller.ReviewAccountingProgram">
    <Dialog horizontalScrolling="false" showHeader="true" state="None" contentWidth="auto" stretch="false" title="{i18n43G>createNewAccSystemLabel}" type="Standard" verticalScrolling="false">
        <content>
			<form:Form width="auto" editable="true" visible="true">
				<form:title/>
				<form:formContainers>
					<form:FormContainer visible="true">
						<form:formElements>
							<form:FormElement visible="true">
								<form:label>
									<Label text="{i18n43G>SystemNameLabel}" design="Standard" width="100%" required="true" textAlign="Begin" textDirection="Inherit" visible="true"/>
								</form:label>
								<form:fields>
									<Text   text="{accountingsystem>system_name}" visible="true"
										width="auto"  />
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="true">
								<form:label>
									<Label text="{i18n43G>SystemContractLabel}" design="Standard" width="100%" required="true" textAlign="Begin" textDirection="Inherit"
										visible="true"/>
								</form:label>
								<form:fields>

									<Link  text=" {path: 'accountingsystem>sys_contract_cmis_id', formatter: '.urlformatter'}" wrapping="true" visible="{= ${accountingsystem>sys_contract_cmis_id} !== '' ? true : false }" target="_blank"
																	href="{accountingsystem>sys_contract_cmis_id}" />

									<Label wrapping="true"/>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="true">
								<form:label>
									<Label text="{i18n43G>SystemGeneratedFinancialBudgetLabel}" design="Standard" width="100%" required="true" textAlign="Begin" textDirection="Inherit"
										visible="true"/>
								</form:label>
								<form:fields>
									<Link  text=" {path: 'accountingsystem>last_quarter_trial_balance_cmis_id', formatter: '.urlformatter'}" wrapping="true" visible="{= ${accountingsystem>last_quarter_trial_balance_cmis_id} !== '' ? true : false }" target="_blank"
																	href="{accountingsystem>last_quarter_trial_balance_cmis_id}" />

									<Label wrapping="true"/>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="true">
								<form:label>
									<Label text="{i18n43G>LastYearLabel}" design="Standard" width="100%" required="true" textAlign="Begin" textDirection="Inherit"
										visible="true"/>
								</form:label>
								<form:fields>
									<Link  text=" {path: 'accountingsystem>last_yearly_trial_balance_cmis_id', formatter: '.urlformatter'}" wrapping="true" visible="{= ${accountingsystem>last_yearly_trial_balance_cmis_id} !== '' ? true : false }" target="_blank"
																	href="{accountingsystem>last_yearly_trial_balance_cmis_id}" />

									<Label wrapping="true"/>
								</form:fields>
							</form:FormElement>

						</form:formElements>
						<form:title/>
					</form:FormContainer>
					<form:FormContainer visible="true">
						<form:formElements>
							<form:FormElement visible="true">
								<form:label>
									<Label text="{i18n43G>SystemGeneratedLabel}" design="Standard" width="100%" required="true" textAlign="Begin" textDirection="Inherit"
										visible="true"/>
								</form:label>
								<form:fields>
									<Link  text=" {path: 'accountingsystem>financial_statement_cmis_id', formatter: '.urlformatter'}" wrapping="true" visible="{= ${accountingsystem>financial_statement_cmis_id} !== '' ? true : false }" target="_blank"
																	href="{accountingsystem>financial_statement_cmis_id}" />

									<Label wrapping="true"/>
								</form:fields>
							</form:FormElement>
							
														<form:FormElement visible="true">
								<form:label>
									<Label text="{i18n43G>StatementofCashFlows}" design="Standard" width="100%" required="true" textAlign="Begin" textDirection="Inherit"
										visible="true"/>
								</form:label>
								<form:fields>
									<Link  text=" {path: 'accountingsystem>StatementofCashFlows', formatter: '.urlformatter'}" wrapping="true" visible="{= ${accountingsystem>StatementofCashFlows} !== '' ? true : false }" target="_blank"
																	href="{accountingsystem>StatementofCashFlows}" />

									<Label wrapping="true"/>
								</form:fields>
							</form:FormElement>
							
							
														<form:FormElement visible="true">
								<form:label>
									<Label text="{i18n43G>Statementoffinancialposition}" design="Standard" width="100%" required="true" textAlign="Begin" textDirection="Inherit"
										visible="true"/>
								</form:label>
								<form:fields>
									<Link  text=" {path: 'accountingsystem>Statementoffinancialposition', formatter: '.urlformatter'}" wrapping="true" visible="{= ${accountingsystem>Statementoffinancialposition} !== '' ? true : false }" target="_blank"
																	href="{accountingsystem>Statementoffinancialposition}" />

									<Label wrapping="true"/>
								</form:fields>
							</form:FormElement>
							
							<form:FormElement visible="true">
								<form:label>
									<Label text="{i18n43G>SystemGeneratedFinancialBudgetLabel}" design="Standard" width="100%" required="true" textAlign="Begin"
										textDirection="Inherit" visible="true"/>
								</form:label>
								<form:fields>
									<Link  text=" {path: 'accountingsystem>budget_statement_cmis_id', formatter: '.urlformatter'}" wrapping="true" visible="{= ${accountingsystem>budget_statement_cmis_id} !== '' ? true : false }" target="_blank"
																	href="{vbudget_statement_cmis_id}" />

									<Label wrapping="true"/>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="true">
								<form:label>
									<Label text="{i18n43G>AttachFinancialStatementGeneratedEmployees}" design="Standard" width="100%" required="true" textAlign="Begin"
										textDirection="Inherit" visible="true"/>
								</form:label>
								<form:fields>
									<Link  text=" {path: 'accountingsystem>list_of_employees_cmis_id', formatter: '.urlformatter'}"  wrapping="true" visible="{= ${accountingsystem>list_of_employees_cmis_id} !== '' ? true : false }" target="_blank"
																	href="{accountingsystem>list_of_employees_cmis_id}" />

									<Label wrapping="true"/>
								</form:fields>
							</form:FormElement>
						</form:formElements>
						<form:title/>
					</form:FormContainer>
				</form:formContainers>
				<form:layout>
					<form:ResponsiveGridLayout adjustLabelSpan="false" columnsL="2" labelSpanL="3" columnsM="1" labelSpanM="2" labelSpanS="12"/>
				</form:layout>
			</form:Form>
        </content>
        <beginButton/>
        <buttons>
            <!--<Button text="{i18n43G>submit}" type="Default" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false" press="_onButtonPress"/>-->
            <Button text="{i18n43G>cancel}" type="Reject" iconFirst="true" width="auto" enabled="true" visible="true" press="_onButtonPress1" iconDensityAware="false"/>
        </buttons>
        <customHeader/>
        <endButton/>
        <subHeader/>
    </Dialog>
</core:FragmentDefinition>